{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
     "indent":"off",
					"linebreak-style": 0,
					"no-console": "off",
					 "eol-last": 0,
						"no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    }
}
